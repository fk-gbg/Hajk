    "express": "^4.18.1",
    "express-openapi-validator": "^4.13.8",
    "helmet": "^6.0.0",
    "http-proxy-middleware": "^2.0.6",
    "log4js": "^6.6.1",
    "node-windows": "^1.0.0-beta.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
